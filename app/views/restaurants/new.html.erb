<h2>add a restaurant</h2>
<%= simple_form_for(@restaurant) do |f| %>
  <%= f.input :name %>
  <%= f.input :address %>
  <%= f.input :phone_number %>
  <%= f.input :category, collection: Restaurant::CATEGORY, input_html: { multiple: true } %>
  <%= f.button :submit, class: 'btn bluebutton' %>
<% end %>
